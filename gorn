const myKey = 'proj-Aixptc7m7JaQdtAQ_ulGSn9jYfo73LPdRKX_jm2yRxAcascjbMf_9AlilAB3ezuYLMnSYNXeulT3BlbkFJHnaj9251a-Mtmslu25mbLs5cm8k9R7URCCUqoLjCjcBSLI5ahewwYHiJhpc8MLdIEcydRW0vMA';   // <-- just the key, with or without sk-


/***********************
 *  DON'T TOUCH BELOW  *
 ***********************/
(async () => {
  const key = myKey.startsWith('sk-') ? myKey : 'sk-' + myKey;
  const res = await fetch('https://api.openai.com/v1/chat/completions', {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json',
      Authorization: `Bearer ${key}`
    },
    body: JSON.stringify({
      model: 'gpt-4',
      messages: [{ role: 'user', content: 'heyo' }],
      temperature: 0.7
    })
  });
  const data = await res.json();
  console.log(data.choices[0].message.content);
})();
